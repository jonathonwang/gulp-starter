{
  "name": "Gulp",
  "icon": "gulp-icon.png",
  "production": false,

  "paths": {
    "vendor": {
      "bootstrap": {
        "sass": "./node_modules/bootstrap-sass/assets/stylesheets",
        "js": "./node_modules/bootstrap-sass/assets/javascripts"
      },
      "jquery": "./node_modules/jquery/dist/jquery.min.js",
      "fontawesome": {
        "sass": "./node_modules/font-awesome/scss",
        "fonts": "./node_modules/font-awesome/fonts"
      }
    },
    "src" : {
      "js": "./src/js",
      "sass": "./src/sass"
    },
    "dist": {
      "js": "./dist/js",
      "css": "./dist/css",
      "html": "./html/**/*.html",
      "fonts": "./dist/fonts"
    }
  }
}
